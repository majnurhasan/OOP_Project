<Window x:Class="OOP_Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OOP_Project"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="140"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid Column="0">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Label Content="Transaction" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="0"></Label>
            <Label Content="Account" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="1"></Label>
            <Label Content="Stocks" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="2"></Label>
            <Label Content="Settings" HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Row="3"></Label>
        </Grid>
    </Grid>
</Window>
